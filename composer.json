{
    "name": "windwalker/framework",
    "description": "The next generation PHP framework.",
    "homepage": "https://github.com/windwalker-io/framework",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-dom": "*",
        "psr/log": "^1.0||^2.0||^3.0",
        "psr/container": "^1.0||^2.0",
        "psr/http-message": "^1.0||^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/cache": "^1.0||^2.0||^3.0",
        "psr/simple-cache": "^1.0||^2.0||^3.0",
        "psr/event-dispatcher": "^1.0",
        "paragonie/constant_time_encoding": "^2.3",
        "composer/ca-bundle": "^1.2",
        "symfony/options-resolver": "^5.0||^6.0||^7.0",
        "symfony/polyfill-php83": "^1.27",
        "asika/object-metadata": "^1.0"
    },
    "require-dev": {
        "asika/sql-splitter": "^1.0",
        "composer/semver": "^3.0",
        "doctrine/inflector": "^2.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "guzzlehttp/promises": "^1.3||^2.0",
        "illuminate/view": "^6.0||^7.0||^8.0||^9.0||^10.0||^11.0",
        "jdorn/sql-formatter": "^1.2",
        "laktak/hjson": "^2.1",
        "league/flysystem": "^2.0",
        "league/plates": "^3.4",
        "masterminds/html5": "^2.7",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.4",
        "mustache/mustache": "^2.0",
        "myclabs/php-enum": "^1.8",
        "phpunit/phpunit": "^10.0||^11.0",
        "react/event-loop": "^1.1",
        "react/http": "^1.0",
        "react/promise": "^2.7",
        "react/socket": "^1.5",
        "reactivex/rxphp": "^2.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.0",
        "swoole/ide-helper": "^4.0||^5.0",
        "symfony/css-selector": "^5.0||^6.0||^7.0",
        "symfony/dom-crawler": "^5.0||^6.0||^7.0",
        "symfony/options-resolver": "^5.0||^6.0||^7.0",
        "symfony/process": "^5.1||^6.0||^7.0",
        "symfony/string": "^5.2||^6.0||^7.0",
        "symfony/var-dumper": "^5.0||^6.0",
        "symfony/yaml": "^5.0||^6.0||^7.0",
        "twig/twig": "^2.0||^3.0",
        "webmozart/glob": "^4.1",
        "yosymfony/toml": "^1.0",
        "laravel/serializable-closure": "^1.0",
        "symfony/polyfill-php82": "^1.26",
        "asika/simple-benchmark": "^3.0",
        "rize/uri-template": "^0.3.5",
        "guzzlehttp/uri-template": "^1.0"
    },
    "suggest": {
        "symfony/yaml": "Install ~4.0 if you require YAML support.",
        "php-http/httplug": "Install ~2.0 if you require HttpClient as HttpPlug implementation.",
        "webmozart/glob": "Install ^4.0 to support Ant-like glob().",
        "ext-openssl": "Install openssl to support legacy openssl cipher.",
        "doctrine/inflector": "Install ~1.0 to use inflector.",
        "aws/aws-sdk-php": "If you want to use AWS SQS as queue service.",
        "iron-io/iron_mq": "If you want to use Iron.io as queue service.",
        "php-amqplib/php-amqplib": "If you want to use RabbitMQ as queue service.",
        "pda/pheanstalk": "If you want to use Beanstalkd queue as service.",
        "chrisboulton/php-resque": "Install <= 1.2 if you want to use PHP Resque (Redis) as queue service.",
        "reactivex/rxphp": "Support EventEmitter::observe() pattern.",
        "relay/relay": "Install ^2.0 to support PSR-15 middlewares.",
        "symfony/string": "Install ^5.2 to use inflector.",
        "laravel/serializable-closure": "Install ^1.0 to support serializable Closure.",
        "ext-sodium": "Install sodium to support sodium cipher.",
        "rize/uri-template": "Install this to support URI Template expand() and extract()",
        "guzzlehttp/uri-template": "Install this to support URI Template expand()"
    },
    "autoload": {
        "psr-4": {
            "Windwalker\\Attributes\\": "packages/attributes/src",
            "Windwalker\\Authentication\\": "packages/authentication/src",
