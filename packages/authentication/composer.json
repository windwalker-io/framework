{
    "name": "windwalker/authentication",
    "type": "windwalker-package",
    "description": "Windwalker Authentication package",
    "keywords": [
        "windwalker",
        "framework",
        "authentication",
        "auth",
        "user"
    ],
    "homepage": "https://github.com/windwalker-io/authentication",
    "license": "MIT",
    "require": {
        "php": ">=8.4.6",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0||^12.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Authentication\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Authentication\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.4.6"
        }
    }
}
