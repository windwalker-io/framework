{
    "name": "windwalker/data",
    "type": "windwalker-package",
    "description": "Windwalker Data package",
    "keywords": [
        "windwalker",
        "framework",
        "utilities"
    ],
    "homepage": "https://github.com/windwalker-io/data",
    "license": "MIT",
    "require": {
        "php": ">=8.2.0",
        "windwalker/scalars": "^4.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/filesystem": "^4.0",
        "windwalker/stream": "^4.0",
        "windwalker/test": "^4.0",
        "symfony/yaml": "^5.0||^6.0||^7.0",
        "laktak/hjson": "^2.1",
        "yosymfony/toml": "^1.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Data\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Data\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
