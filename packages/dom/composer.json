{
    "name": "windwalker/dom",
    "type": "windwalker-package",
    "description": "Windwalker DOM package",
    "keywords": [
        "windwalker",
        "framework",
        "dom"
    ],
    "homepage": "https://github.com/windwalker-io/dom",
    "license": "MIT",
    "require": {
        "php": ">=8.2.0",
        "windwalker/utilities": "^4.0",
        "ext-json": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/test": "^4.0",
        "masterminds/html5": "^2.7",
        "symfony/dom-crawler": "^5.0||^6.0||^7.0",
        "symfony/css-selector": "^5.0||^6.0||^7.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\DOM\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\DOM\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
