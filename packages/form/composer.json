{
    "name": "windwalker/form",
    "type": "windwalker-package",
    "description": "Windwalker Form package",
    "keywords": [
        "windwalker",
        "framework",
        "form",
        "form-builder",
        "html"
    ],
    "homepage": "https://github.com/windwalker-io/form",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "windwalker/attributes": "^4.0",
        "windwalker/filter": "^4.0",
        "windwalker/data": "^4.0",
        "windwalker/dom": "^4.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Form\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Form\\Test\\": "test/"
        }
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Form\\FormPackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
