{
    "name": "windwalker/html",
    "type": "windwalker-package",
    "description": "Windwalker Html package",
    "keywords": [
        "windwalker",
        "framework",
        "html",
        "html-builder"
    ],
    "homepage": "https://github.com/windwalker-io/html",
    "license": "MIT",
    "require": {
        "php": ">=8.2.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Html\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Html\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "plathtml": {
            "php": "8.0.0"
        }
    }
}
