{
    "name": "windwalker/language",
    "type": "windwalker-package",
    "description": "Windwalker Language package",
    "keywords": [
        "windwalker",
        "framework",
        "language",
        "translation",
        "multi-language"
    ],
    "homepage": "https://github.com/windwalker-io/language",
    "license": "MIT",
    "require": {
        "php": ">=8.4.6",
        "windwalker/data": "^4.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0||^12.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Language\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Language\\Test\\": "test/"
        }
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Language\\LanguagePackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platlanguage": {
            "php": "8.4.1"
        }
    }
}
