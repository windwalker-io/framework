{
    "name": "windwalker/orm",
    "type": "windwalker-package",
    "description": "Windwalker ORM package",
    "keywords": [
        "windwalker",
        "framework",
        "orm",
        "database",
        "datamapper",
        "mysql",
        "postgresql",
        "sqlsrv",
        "pdo"
    ],
    "homepage": "https://github.com/windwalker-io/orm",
    "license": "MIT",
    "require": {
        "php": ">=8.4.6",
        "asika/object-metadata": "^1.0",
        "windwalker/attributes": "^4.0",
        "windwalker/database": "^4.0",
        "windwalker/utilities": "^4.0",
        "symfony/polyfill-php83": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0||^12.0",
        "windwalker/test": "^4.0",
        "myclabs/php-enum": "^1.8"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\ORM\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\ORM\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.4.6"
        }
    }
}
