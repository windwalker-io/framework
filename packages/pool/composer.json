{
    "name": "windwalker/pool",
    "type": "windwalker-package",
    "description": "Windwalker Pool package",
    "keywords": [
        "windwalker",
        "framework",
        "connection pool"
    ],
    "homepage": "https://github.com/windwalker-io/pool",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "symfony/options-resolver": "^5.0||^6.0",
        "windwalker/reactor": "^4.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Pool\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Pool\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platpool": {
            "php": "8.0.0"
        }
    }
}
