{
    "name": "windwalker/promise",
    "type": "windwalker-package",
    "description": "Windwalker Promise package",
    "keywords": [
        "windwalker",
        "framework",
        "promise",
        "swoole",
        "async",
        "reactor",
        "react",
        "coroutine"
    ],
    "homepage": "https://github.com/windwalker-io/promise",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/reactor": "^4.0",
        "windwalker/test": "^4.0",
        "swoole/ide-helper": "^4.0||^5.0",
        "guzzlehttp/promises": "^1.3",
        "react/promise": "^2.7",
        "react/event-loop": "^1.1"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Promise\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Promise\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
