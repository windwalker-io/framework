{
    "name": "windwalker/reactor",
    "type": "windwalker-package",
    "description": "Windwalker Swoole package",
    "keywords": [
        "windwalker",
        "framework",
        "swoole",
        "react",
        "async",
        "reactor"
    ],
    "homepage": "https://github.com/windwalker-io/swoole",
    "license": "MIT",
    "require": {
        "php": ">=8.4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0||^12.0",
        "windwalker/test": "^4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Reactor\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Reactor\\Test\\": "test/"
        }
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Reactor\\ReactorPackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.4.6"
        }
    }
}
