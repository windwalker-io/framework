{
    "name": "windwalker/renderer",
    "type": "windwalker-package",
    "description": "Windwalker Renderer package",
    "keywords": [
        "windwalker",
        "framework",
        "renderer",
        "blade",
        "edge",
        "mustach",
        "plates"
    ],
    "homepage": "https://github.com/windwalker-io/renderer",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/test": "^4.0",
        "windwalker/dom": "^4.0",
        "windwalker/filesystem": "^4.0",
        "twig/twig": "^2.0",
        "illuminate/view": "^6.0||^7.0||^8.0||^9.0||^10.0",
        "mustache/mustache": "^2.0",
        "league/plates": "^3.4"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Renderer\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Renderer\\Test\\": "test/"
        }
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Renderer\\RendererPackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
