{
    "name": "windwalker/scalars",
    "type": "windwalker-package",
    "description": "Windwalker Scalars package",
    "keywords": [
        "windwalker",
        "framework",
        "utilities",
        "scalar object",
        "array object"
    ],
    "homepage": "https://github.com/windwalker-io/scalars",
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "windwalker/data": "^4.0",
        "windwalker/test": "^4.0",
        "doctrine/inflector": "^2.0"
    },
    "suggest": {
        "doctrine/inflector": "Install ~1.0 to use inflector."
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Scalars\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Scalars\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
