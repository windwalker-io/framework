{
    "name": "windwalker/session",
    "type": "windwalker-package",
    "description": "Windwalker Session package",
    "keywords": [
        "windwalker",
        "framework",
        "session"
    ],
    "homepage": "https://github.com/windwalker-io/session",
    "license": "MIT",
    "require": {
        "php": ">=8.4.1",
        "windwalker/cache": "^4.0",
        "windwalker/utilities": "^4.0"
    },
    "require-dev": {
        "asika/sql-splitter": "^1.0",
        "phpunit/phpunit": "^8.0||^9.0||^10.0",
        "mockery/mockery": "^1.4",
        "windwalker/test": "^4.0",
        "windwalker/data": "^4.0",
        "windwalker/database": "^4.0",
        "windwalker/filesystem": "^4.0",
        "mikey179/vfsstream": "^1.6",
        "webmozart/glob": "^4.1"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Session\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Session\\Test\\": "test/"
        }
    },
    "extra": {
        "windwalker": {
            "packages": [
                "Windwalker\\Session\\SessionPackage"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.4.1"
        }
    }
}
