{
    "name": "windwalker/test",
    "type": "windwalker-package",
    "description": "Windwalker Test package",
    "keywords": [
        "windwalker",
        "framework",
        "test"
    ],
    "homepage": "https://github.com/windwalker-io/test",
    "license": "MIT",
    "require": {
        "php": ">=8.4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0||^11.0||^12.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Test\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Test\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.4.1"
        }
    }
}
